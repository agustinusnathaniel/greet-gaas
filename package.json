{
  "name": "greeting-gaas",
  "version": "1.0.1",
  "private": true,
  "author": "agustinusnathaniel",
  "engines": {
    "node": "^24.11.x"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "turbo": "pnpm dlx turbo@2.6.3",
    "build:turbo": "pnpm turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "biome:check": "biome check --no-errors-on-unmatched --error-on-warnings",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "type:check": "tsc --noEmit",
    "check:turbo": "pnpm turbo run biome:check type:check",
    "start": "next start",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.3",
    "@types/crypto-js": "^4.2.1",
    "axios": "^1.6.3",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.23.26",
    "next": "^16.0.10",
    "next-seo": "^6.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.49.2",
    "react-icons": "^5.5.0",
    "react-wrap-balancer": "^1.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-sitemap": "^4.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3"
  }
}
